                "${workspaceFolder}/**",
                "/opt/homebrew/include",
                "/opt/homebrew/include/boost",
                "/opt/homebrew/include/eigen3",
                "/opt/homebrew/Cellar/eigen/3.4.0_1/include/eigen3"
            ],
            "defines": [],
